# Esempio di dhtput per caricare le firme
./twisterd -genproclimit=1 -rpcuser=user -rpcpassword=pwd -rpcallowip=127.0.0.1 -rpcport=48001 dhtput utente1 signature s '["asd","assd"]' utente1 0 
./twisterd -genproclimit=1 -rpcuser=user -rpcpassword=pwd -rpcallowip=127.0.0.1 -rpcport=48001 dhtget utente1 signature                            
